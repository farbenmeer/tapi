{
  "name": "@farbenmeer/bunny",
  "version": "0.4.1",
  "author": {
    "name": "Michel Smola",
    "email": "michel.smola@farbenmeer.de"
  },
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farbenmeer/tapi.git"
  },
  "files": [
    "dist",
    "boilerplate.tar.gz",
    "recipes"
  ],
  "bin": {
    "bunny": "./cli.js",
    "@farbenmeer/bunny": "./cli.js"
  },
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --noEmit false",
    "pack:boilerplate": "pnpm --dir=\"../4-bunny-boilerplate\" pack --out=\"../3-bunny/boilerplate.tar.gz\"",
    "cli": "node dist/cli.js",
    "release": "pnpm run build && pnpm run pack:boilerplate && pnpm publish --no-git-checks",
    "test": "vitest"
  },
  "dependencies": {
    "@farbenmeer/react-tapi": "workspace:^",
    "@farbenmeer/router": "workspace:^",
    "@farbenmeer/tapi": "workspace:^",
    "commander": "^14.0.0",
    "connect": "^3.7.0",
    "esbuild": "^0.27.2",
    "serve-static": "^2.2.1",
    "tar": "^7.5.4",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "devDependencies": {
    "@types/connect": "^3.4.38",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19",
    "@types/serve-static": "^2.2.0",
    "node-mocks-http": "^1.17.2",
    "react": "^19.2.3",
    "react-dom": "^19",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19",
    "typescript": "^5"
  }
}
